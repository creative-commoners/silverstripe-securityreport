{
    "name": "silverstripe/securityreport",
    "description": "Generates a security and access report for CMS users",
    "keywords": ["silverstripe", "security", "report"],
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4@dev",
        "silverstripe/cms": "^4@dev",
        "silverstripe/reports": "^4@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra":  {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SecurityReport\\": "src/",
            "SilverStripe\\SecurityReport\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
